<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
	"http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	
	<!-- 设置系统参数 -->
	<constant name="struts.devMode" value="true"/>
	<constant name="struts.action.extension" value="action,do,,"/>
	<constant name="struts.ui.theme" value="simple"/>

	<package name="yl-productType" extends="json-default">
		<global-allowed-methods>regex:.*</global-allowed-methods>
		<action name="productTypeAction_*" method="{1}" class="productTypeAction">
			<result name="error" type="redirect">/index.jsp</result>
            <result name="findProductTypeList" type="json">
                <param name="includeProperties">productTypeList.*</param>
            </result>
		</action>
	</package>
</struts>





